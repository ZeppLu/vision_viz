<launch>
  <node name="detections_viz" pkg="vision_viz" type="detections_viz" output="screen">
    <remap from="/detections_viz/image_in/compressed" to="/jetbot_camera/raw/compressed"/>
    <remap from="/detections_viz/detections" to="/detectnet/detections"/>
    <param name="detect_delay_lb_ms" value="200"/>
    <param name="age_penalty" value="1.0"/>
  </node>
</launch>
